.env
.env.local
.env.*.local
.env.build
.env.test
.env.production

# Ignore package manager files
package-lock.json
yarn.lock
.pnpm-lock.yaml
.pnpm

# Ignore version control system directories
.git
.svn
.github

.vscode
.idea
.gitignore
.prettier*
Dockerfile
docker-compose.yaml
